<% if move.user_id == current_user.id %>
    <tr>
      <td class="mdl-data-table__cell--non-numeric"><%= move.name %></td>
      <% if move.sentido == "entrada" %>
          <td class="entrada">  <%= number_to_currency(move.value, scale: 2) %> </td>
          <td></td>
      <%	else  %>
          <td></td>
          <td class="despesa">  <%= number_to_currency(move.value, scale: 2) %> </td>
      <% end %>
      <td>  <%= I18n.l (move.created_at), format: :short %> </td>
      <td>  <i> <%= link_to 'pageview', move, class: "material-icons", style: "text-decoration: none" %> </i>
      <i> <%= link_to 'delete', move, method: :delete, class: "material-icons delete_move",
                      style: "text-decoration: none", data: { confirm: 'Are you sure?' }, remote: true %> </i> </td>
    </tr>
<% end %>